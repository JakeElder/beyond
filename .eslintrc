{
  "parser": "babel-eslint",
  "extends": "airbnb",
  "env": {
    "browser": true,
    "node": true
  },
  "globals": {
    "__DEV__": true,
    "__BROWSER__": true
  },
  "rules": {
    "semi": 0,
    "react/jsx-filename-extension": 0,
    "react/forbid-prop-types": 0,
    "no-underscore-dangle": 0,
    "comma-dangle": 0,
    "import/no-extraneous-dependencies": [
      "error", {
         "packageDir": "./"
      }
    ],
    "import/prefer-default-export": 0
  }

}
